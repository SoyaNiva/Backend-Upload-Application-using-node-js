{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/upload",
            "dest": "/api/upload"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
